{
  "name": "server-template",
  "version": "1.0.0",
  "description": "Server Template",
  "engineStrict": true,
  "engines": {
    "node": ">= 8.9.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.4",
    "@types/express": "4.17.0",
    "@types/jest": "^23.3.1",
    "@types/node": "12.12.7",
    "@types/request-promise": "^4.1.42",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^3.4.4",
    "babel-jest": "^26.0.1",
    "jest": "26.0.1",
    "minimist": "^1.2.0",
    "nodemon": "1.18.3",
    "replace-in-file": "^3.4.2",
    "supertest": "^6.0.0",
    "ts-jest": "26.1.0",
    "ts-node": "^7.0.0",
    "tsconfig-paths": "^3.5.0",
    "tslint": "^5.20.1",
    "tslint-microsoft-contrib": "5.1.0",
    "tsutils": "3.0.0",
    "typescript": "3.9.5"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp dist",
    "build": "npm run clean && npm run build-src && npm run fix-ts",
    "build-src": "tsc -p tsconfig.release.json",
    "watch": "tsc -w -p tsconfig.release.json",
    "run": "nodemon --inspect=5858 ./src/main.ts",
    "debug": "ts-node --inspect=9001 -r tsconfig-paths/register src/main.ts",
    "lint": "tslint -t stylish --project \"tsconfig.json\" --force",
    "pretest": "npm run lint",
    "test": "npm run test-only",
    "test-only": "jest",
    "fix-ts": "node utils/tsresolve.js",
    "test:watch": "jest --watch"
  },
  "author": "KarmaDev Technology",
  "license": "Limited",
  "repository": {},
  "dependencies": {
    "@types/multer": "^1.4.4",
    "@types/node-fetch": "^2.5.7",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "4.17.1",
    "multer": "^1.4.2",
    "querystring": "^0.2.0",
    "reflect-metadata": "0.1.12",
    "request": "2.88.0",
    "request-promise": "^4.2.2",
    "rimraf": "^3.0.2",
    "swagger-ui-express": "^4.1.4",
    "tslib": "1.9.3",
    "uuid": "^3.3.3"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  }
}
